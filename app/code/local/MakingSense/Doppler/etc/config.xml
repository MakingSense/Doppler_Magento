<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modules>
		<MakingSense_Doppler>
			<version>0.1.2</version>
		</MakingSense_Doppler>
	</modules>
	<admin>
		<routers>
			<adminhtml>
				<args>
					<modules>
						<MakingSense_Doppler before="Mage_Adminhtml">MakingSense_Doppler_Adminhtml</MakingSense_Doppler>
					</modules>
				</args>
			</adminhtml>
		</routers>
	</admin>
	<global>
		<helpers>
			<makingsense_doppler>
				<class>MakingSense_Doppler_Helper</class>
			</makingsense_doppler>
